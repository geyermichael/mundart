{
  "name": "mundart",
  "version": "0.1.0",
  "description": "Simple i18n web editor for developers",
  "private": true,
  "type": "module",
  "keywords": [
    "i18n",
    "editor",
    "internationalization",
    "localization"
  ],
  "author": {
    "name": "Michael Geyer",
    "email": "michael@qbitone.de"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geyermichael/mundart"
  },
  "license": "MIT",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "playground": "PLAYGROUND=true pnpm build && cd playground && NUXT_LOCALE_DIR_PATH=src/i18n pnpm run mundart",
    "dist:publish": "pnpm build && cd dist && npm publish"
  },
  "dependencies": {
    "@nuxt/eslint": "0.3.13",
    "@nuxt/ui": "2.17.0",
    "nuxt": "3.12.3"
  }
}
